// Generated by Selenium IDE
const { Builder, By, Key, until } = require('selenium-webdriver')
const assert = require('assert')

describe('Convert', function() {
  this.timeout(30000)
  let driver
  let vars
  beforeEach(async function() {
    driver = await new Builder().forBrowser('chrome').build()
    vars = {}
  })
  afterEach(async function() {
    await driver.quit();
  })
  it('Convert', async function() {
    await driver.get("http://localhost:3000/")
    await driver.manage().window().setRect({ width: 958, height: 816 })
    await driver.findElement(By.id("value")).click()
    await driver.findElement(By.id("value")).sendKeys("hello")
    await driver.findElement(By.css("button:nth-child(5)")).click()
    await driver.findElement(By.css("button:nth-child(6)")).click()
    await driver.findElement(By.id("sourceType")).click()
    {
      const dropdown = await driver.findElement(By.id("sourceType"))
      await dropdown.findElement(By.xpath("//option[. = 'Binary']")).click()
    }
    await driver.findElement(By.id("destType")).click()
    {
      const dropdown = await driver.findElement(By.id("destType"))
      await dropdown.findElement(By.xpath("//option[. = 'Octal']")).click()
    }
    await driver.findElement(By.id("value")).click()
    await driver.findElement(By.id("value")).sendKeys("01101000 01100101 01101100 01101100 01101111 ")
    await driver.findElement(By.css("button:nth-child(5)")).click()
    await driver.findElement(By.css("button:nth-child(6)")).click()
    await driver.findElement(By.id("sourceType")).click()
    {
      const dropdown = await driver.findElement(By.id("sourceType"))
      await dropdown.findElement(By.xpath("//option[. = 'Octal']")).click()
    }
    await driver.findElement(By.id("destType")).click()
    {
      const dropdown = await driver.findElement(By.id("destType"))
      await dropdown.findElement(By.xpath("//option[. = 'ASCII']")).click()
    }
    await driver.findElement(By.id("value")).click()
    await driver.findElement(By.id("value")).sendKeys("150 145 154 154 157 ")
    await driver.findElement(By.css("button:nth-child(5)")).click()
    await driver.findElement(By.id("sourceType")).click()
    {
      const dropdown = await driver.findElement(By.id("sourceType"))
      await dropdown.findElement(By.xpath("//option[. = 'ASCII']")).click()
    }
    await driver.findElement(By.id("destType")).click()
    {
      const dropdown = await driver.findElement(By.id("destType"))
      await dropdown.findElement(By.xpath("//option[. = 'Octal']")).click()
    }
    await driver.findElement(By.css("form")).click()
    await driver.findElement(By.id("value")).sendKeys("hello")
    await driver.findElement(By.css("button:nth-child(5)")).click()
    await driver.findElement(By.css("button:nth-child(6)")).click()
    await driver.findElement(By.id("destType")).click()
    {
      const dropdown = await driver.findElement(By.id("destType"))
      await dropdown.findElement(By.xpath("//option[. = 'Binary']")).click()
    }
    await driver.findElement(By.id("sourceType")).click()
    {
      const dropdown = await driver.findElement(By.id("sourceType"))
      await dropdown.findElement(By.xpath("//option[. = 'Octal']")).click()
    }
    await driver.findElement(By.id("value")).click()
    await driver.findElement(By.id("value")).sendKeys("150 145 154 154 157 ")
    await driver.findElement(By.css("button:nth-child(5)")).click()
    await driver.findElement(By.css("button:nth-child(6)")).click()
    await driver.findElement(By.id("sourceType")).click()
    {
      const dropdown = await driver.findElement(By.id("sourceType"))
      await dropdown.findElement(By.xpath("//option[. = 'Binary']")).click()
    }
    await driver.findElement(By.id("destType")).click()
    {
      const dropdown = await driver.findElement(By.id("destType"))
      await dropdown.findElement(By.xpath("//option[. = 'ASCII']")).click()
    }
    await driver.findElement(By.id("value")).click()
    await driver.findElement(By.id("value")).sendKeys("01101000 01100101 01101100 01101100 01101111 ")
    await driver.findElement(By.css("button:nth-child(5)")).click()
  })
})
